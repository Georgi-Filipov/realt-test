{
  "$schema": "http://json.schemastore.org/prettierrc",
  "printWidth": 120,
  "proseWrap": "always",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": true,
  "endOfLine": "lf",
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "avoid"
}
